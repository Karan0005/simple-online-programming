{
    "name": "@full-stack-project/source",
    "version": "0.0.0",
    "license": "MIT",
    "scripts": {
        "dependency:verifier": "node scripts/dependency.verifier.js",
        "install": "npm run make:env && npm run dependency:verifier && npm run install:extensions",
        "install:extensions": "node scripts/vs.extensions.setup.js",
        "make:env": "node scripts/env.builder.js",
        "upgrade": "node scripts/upgrade.js",
        "docker:up": "docker-compose --env-file .env build --no-cache && docker-compose --env-file .env up",
        "docker:clear": "docker-compose down --rmi all --volumes --remove-orphans && docker system prune -a --volumes -f",
        "clear:cache": "npm cache clean --force && npx nx reset",
        "generate:module": "node scripts/generate.module.js",
        "frontend:lint": "prettier apps/frontend/**/*.{js,ts,scss,html,json} --write  && eslint apps/frontend/**/*.{ts,js} --fix",
        "frontend:build": "node scripts/frontend.ignite.js build",
        "frontend:test": "nx test frontend --skip-nx-cache",
        "frontend:e2e": "nx e2e frontend-e2e --skip-nx-cache",
        "frontend:serve": "node scripts/frontend.ignite.js serve",
        "backend:lint": "prettier apps/backend/**/*.{js,ts,scss,html,json} --write  && eslint apps/backend/**/*.{ts,js} --fix",
        "backend:build": "npm run backend:lint && npm run backend:test && nx build backend --skip-nx-cache",
        "backend:test": "nx test backend --skip-nx-cache",
        "backend:e2e": "nx e2e backend-e2e --skip-nx-cache",
        "backend:serve": "nx serve backend --skip-nx-cache"
    },
    "private": true,
